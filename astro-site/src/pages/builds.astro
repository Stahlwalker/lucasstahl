---
import Layout from '../layouts/Layout.astro';
---

<Layout
	title="Builds - Luke Stahl"
	description="Builds from Luke Stahl â€” showcasing work across developer marketing, product strategy, and modern web development."
	canonicalUrl="https://lukestahl.io/builds/"
	ogTitle="Builds - Luke Stahl"
	ogDescription="A collection of projects, experiments, and digital experiences built with React, Next.js, TypeScript, and modern web technologies."
	additionalCss={["/assets/css/builds.css"]}
>
	<!-- Builds Hero Section -->
	<section class="builds-hero">
		<div class="section-container">
			<h1 class="builds-hero-title">
				<span class="code-bracket">&lt;</span>
				Builds
				<span class="code-bracket">/&gt;</span>
			</h1>
			<p class="builds-hero-subtitle">A collection of projects, experiments, and digital experiences</p>
		</div>
	</section>

	<!-- Builds Grid Section -->
	<section class="builds-section">
		<div class="section-container">
			<div class="builds-grid">

				<!-- Project: Developer Marketing Claude Skill (Featured) -->
				<article class="build-card featured-project">
					<a href="https://github.com/Stahlwalker/developer-marketing" target="_blank" rel="noopener noreferrer" class="build-card-link">
						<div class="build-card-content">
							<div class="build-icon featured-icon">
								<i class="fa-solid fa-brain"></i>
							</div>
							<h2 class="build-title">Developer Marketing Claude Skill</h2>
							<p class="build-description">
								Custom Claude skill that teaches AI agents developer marketing strategies with templates and a developer writing style guideline.
							</p>
							<div class="build-tags">
								<span class="tag">Claude</span>
								<span class="tag">AI</span>
								<span class="tag">Developer Marketing</span>
								<span class="tag">Skill</span>
							</div>
							<div class="build-link-wrapper">
								<span class="build-link-text">View Project <i class="fa-solid fa-arrow-up-right-from-square"></i></span>
							</div>
						</div>
					</a>
				</article>

				<!-- Project: Newsletter Builder -->
		<article class="build-card">
			<div class="build-card-content">
				<div class="build-icon">
					<i class="fa-solid fa-newspaper"></i>
				</div>
				<h2 class="build-title">Newsletter Builder</h2>
				<p class="build-description">
					Newsletter curation tool that streamlines creating, reviewing, and sending link-based newsletters.
				</p>
				<div class="build-tags">
					<span class="tag">React</span>
					<span class="tag">Vite</span>
					<span class="tag">Node.js</span>
					<span class="tag">PostgreSQL</span>
				</div>
				<div class="build-link-wrapper dual-links">
					<a href="https://marketing-builder-ui.onrender.com/" target="_blank" rel="noopener noreferrer" class="build-link-text">
						View Demo <i class="fa-solid fa-arrow-up-right-from-square"></i>
					</a>
					<a href="https://github.com/Stahlwalker/newsletter-builder" target="_blank" rel="noopener noreferrer" class="build-link-text">
						GitHub <i class="fa-solid fa-arrow-up-right-from-square"></i>
					</a>
				</div>
			</div>
		</article>

		<!-- Project: Family Gaming Hub -->
				<article class="build-card">
					<a href="https://family-gaming.netlify.app/" target="_blank" rel="noopener noreferrer" class="build-card-link">
						<div class="build-card-content">
							<div class="build-icon">
								<i class="fa-solid fa-gamepad"></i>
							</div>
							<h2 class="build-title">Family Gaming Hub</h2>
							<p class="build-description">
								A nostalgic gaming hub built with React and Vite featuring a classic Mac OS 9 inspired interface and retro design aesthetic.
							</p>
							<div class="build-tags">
								<span class="tag">React</span>
								<span class="tag">Vite</span>
								<span class="tag">Tailwind CSS</span>
							</div>
							<div class="build-link-wrapper">
								<span class="build-link-text">View Project <i class="fa-solid fa-arrow-up-right-from-square"></i></span>
							</div>
						</div>
					</a>
				</article>

				<!-- Project: Budget Tracker App -->
				<article class="build-card">
					<a href="https://budget-app-cv27.vercel.app/" target="_blank" rel="noopener noreferrer" class="build-card-link">
						<div class="build-card-content">
							<div class="build-icon">
								<i class="fa-solid fa-chart-pie"></i>
							</div>
							<h2 class="build-title">Budget Tracker App</h2>
							<p class="build-description">
								Expense tracking app built with React Native and Expo. Uses Google OAuth, Supabase sync, flexible time ranges, and simple charts.
							</p>
							<div class="build-tags">
								<span class="tag">React Native</span>
								<span class="tag">Expo</span>
								<span class="tag">TypeScript</span>
								<span class="tag">Supabase</span>
							</div>
							<div class="build-link-wrapper">
								<span class="build-link-text">View Project <i class="fa-solid fa-arrow-up-right-from-square"></i></span>
							</div>
						</div>
					</a>
				</article>

				<!-- Project: Fantasy Football Legend -->
				<article class="build-card">
					<a href="https://stahlwalker.github.io/fantasyfootball/" target="_blank" rel="noopener noreferrer" class="build-card-link">
						<div class="build-card-content">
							<div class="build-icon">
								<i class="fa-solid fa-football"></i>
							</div>
							<h2 class="build-title">Fantasy Football Legend</h2>
							<p class="build-description">
								A showcase website for a Fantasy Football League built with HTML, CSS, and JavaScript.
							</p>
							<div class="build-tags">
								<span class="tag">HTML</span>
								<span class="tag">CSS</span>
								<span class="tag">JavaScript</span>
							</div>
							<div class="build-link-wrapper">
								<span class="build-link-text">View Project <i class="fa-solid fa-arrow-up-right-from-square"></i></span>
							</div>
						</div>
					</a>
				</article>

				<!-- Project: SERP Tracker -->
				<article class="build-card">
					<a href="https://seo-rank-tracker-iota.vercel.app/" target="_blank" rel="noopener noreferrer" class="build-card-link">
						<div class="build-card-content">
							<div class="build-icon">
								<i class="fa-solid fa-magnifying-glass"></i>
							</div>
							<h2 class="build-title">SERP Tracker</h2>
							<p class="build-description">
								Real-time search engine ranking tracker built with React and TypeScript. Features Chart.js visualizations, TanStack Table for data display, and SERP API integration.
							</p>
							<div class="build-tags">
								<span class="tag">React</span>
								<span class="tag">TypeScript</span>
								<span class="tag">Chart.js</span>
								<span class="tag">Tailwind CSS</span>
							</div>
							<div class="build-link-wrapper">
								<span class="build-link-text">View Project <i class="fa-solid fa-arrow-up-right-from-square"></i></span>
							</div>
						</div>
					</a>
				</article>

				<!-- Project: Stahlwalker Cookbook -->
				<article class="build-card">
					<a href="https://cookblog.vercel.app/" target="_blank" rel="noopener noreferrer" class="build-card-link">
						<div class="build-card-content">
							<div class="build-icon">
								<i class="fa-solid fa-utensils"></i>
							</div>
							<h2 class="build-title">Stahlwalker Cookbook</h2>
							<p class="build-description">
								Full-stack recipe app with Next.js and Contentful CMS featuring JAMstack architecture and dynamic content delivery.
							</p>
							<div class="build-tags">
								<span class="tag">Next.js</span>
								<span class="tag">React</span>
								<span class="tag">Contentful</span>
							</div>
							<div class="build-link-wrapper">
								<span class="build-link-text">View Project <i class="fa-solid fa-arrow-up-right-from-square"></i></span>
							</div>
						</div>
					</a>
				</article>

				<!-- Project: Shacks Bloom & Co -->
				<article class="build-card">
					<a href="https://bloom-ebon-two.vercel.app/" target="_blank" rel="noopener noreferrer" class="build-card-link">
						<div class="build-card-content">
							<div class="build-icon">
								<i class="fa-solid fa-heart"></i>
							</div>
							<h2 class="build-title">Shacks Bloom & Co</h2>
							<p class="build-description">
								Web application built with React and Vite, integrated with Builder.io's Fusion AI tool for visual editing across non-technical team members and managed through GitHub.
							</p>
							<div class="build-tags">
								<span class="tag">React</span>
								<span class="tag">Vite</span>
								<span class="tag">JavaScript</span>
								<span class="tag">Builder.io</span>
							</div>
							<div class="build-link-wrapper">
								<span class="build-link-text">View Project <i class="fa-solid fa-arrow-up-right-from-square"></i></span>
							</div>
						</div>
					</a>
				</article>

				<!-- Project: Developer Hub -->
				<article class="build-card">
					<a href="https://developer-hub-webflow.vercel.app/" target="_blank" rel="noopener noreferrer" class="build-card-link">
						<div class="build-card-content">
							<div class="build-icon">
								<i class="fa-solid fa-code"></i>
							</div>
							<h2 class="build-title">Developer Hub</h2>
							<p class="build-description">
								Inspiration for what a modern developer portal should include, built with Vite, React, and TypeScript. Features Framer Motion animations, Tailwind CSS styling, and syntax highlighting with Prism.
							</p>
							<div class="build-tags">
								<span class="tag">Vite</span>
								<span class="tag">React</span>
								<span class="tag">TypeScript</span>
								<span class="tag">Tailwind CSS</span>
							</div>
							<div class="build-link-wrapper">
								<span class="build-link-text">View Project <i class="fa-solid fa-arrow-up-right-from-square"></i></span>
							</div>
						</div>
					</a>
				</article>

				<!-- Project: Packers Nation Invasion -->
				<article class="build-card">
					<a href="https://stahlwalker.github.io/greenbaypackers/" target="_blank" rel="noopener noreferrer" class="build-card-link">
						<div class="build-card-content">
							<div class="build-icon">
								<i class="fa-solid fa-location-dot"></i>
							</div>
							<h2 class="build-title">Packers Nation Invasion</h2>
							<p class="build-description">
								A React-based website documenting the yearly journey of supporting the Green Bay Packers at road games across the NFL.
							</p>
							<div class="build-tags">
								<span class="tag">React</span>
								<span class="tag">Vite</span>
								<span class="tag">Tailwind CSS</span>
								<span class="tag">Shadcn</span>
							</div>
							<div class="build-link-wrapper">
								<span class="build-link-text">View Project <i class="fa-solid fa-arrow-up-right-from-square"></i></span>
							</div>
						</div>
					</a>
				</article>

				<!-- Project: Stahlwalker.org -->
				<article class="build-card">
					<a href="https://stahlwalker.org/" target="_blank" rel="noopener noreferrer" class="build-card-link">
						<div class="build-card-content">
							<div class="build-icon">
								<i class="fa-solid fa-globe"></i>
							</div>
							<h2 class="build-title">Stahlwalker.org</h2>
							<p class="build-description">
								My first portfolio site built with Jekyll, a static site generator. Features a clean design with blog integration and project showcase.
							</p>
							<div class="build-tags">
								<span class="tag">Jekyll</span>
								<span class="tag">Ruby</span>
								<span class="tag">Portfolio</span>
							</div>
							<div class="build-link-wrapper">
								<span class="build-link-text">View Project <i class="fa-solid fa-arrow-up-right-from-square"></i></span>
							</div>
						</div>
					</a>
				</article>

				<!-- Project: Portfolio Template -->
				<article class="build-card">
					<a href="https://stahlwalker.github.io/materialize/" target="_blank" rel="noopener noreferrer" class="build-card-link">
						<div class="build-card-content">
							<div class="build-icon">
								<i class="fa-solid fa-user"></i>
							</div>
							<h2 class="build-title">Portfolio Template</h2>
							<p class="build-description">
								Single page portfolio template created using Materialize CSS. A clean, responsive design showcasing work and skills.
							</p>
							<div class="build-tags">
								<span class="tag">Materialize</span>
								<span class="tag">CSS</span>
								<span class="tag">Template</span>
							</div>
							<div class="build-link-wrapper">
								<span class="build-link-text">View Project <i class="fa-solid fa-arrow-up-right-from-square"></i></span>
							</div>
						</div>
					</a>
				</article>

			</div>

			<!-- Early Projects Section -->
			<div class="early-projects-section">
				<h2 class="section-title">
					<span class="section-label">/ TINY APPS</span>
				</h2>
				<p class="section-description">Tinkering and learning over the years with simple apps</p>

				<div class="projects-list">
					<a href="https://lukes-competitive-web-scraper.webflow.io/website-scraper" target="_blank" rel="noopener noreferrer" class="project-row">
						<div class="project-square"></div>
						<div class="project-info">
							<h3 class="project-name">Market Radar</h3>
							<p class="project-desc">A Webflow App Gen project that scrapes competitor sites and uses Resend for scheduled monitoring.</p>
						</div>
						<div class="project-arrow">
							<i class="fa-solid fa-arrow-up-right-from-square"></i>
						</div>
					</a>

					<a href="https://dev-writing-assistant.netlify.app/" target="_blank" rel="noopener noreferrer" class="project-row">
						<div class="project-square"></div>
						<div class="project-info">
							<h3 class="project-name">Developer Writing Assistant</h3>
							<p class="project-desc">AI-powered writing assistant that transforms content into Luke's developer writing style.</p>
						</div>
						<div class="project-arrow">
							<i class="fa-solid fa-arrow-up-right-from-square"></i>
						</div>
					</a>

					<a href="https://face-app-80d9450b.netlify.app/" target="_blank" rel="noopener noreferrer" class="project-row">
						<div class="project-square"></div>
						<div class="project-info">
							<h3 class="project-name">Face Recognition App</h3>
							<p class="project-desc">Face detection application built with React and Netlify Functions using Clarifai AI API</p>
						</div>
						<div class="project-arrow">
							<i class="fa-solid fa-arrow-up-right-from-square"></i>
						</div>
					</a>

					<a href="https://stahlwalker.github.io/Brewary-Project/" target="_blank" rel="noopener noreferrer" class="project-row">
						<div class="project-square"></div>
						<div class="project-info">
							<h3 class="project-name">BrewIary App</h3>
							<p class="project-desc">A brewery app to search beer and log notes with Firebase authentication</p>
						</div>
						<div class="project-arrow">
							<i class="fa-solid fa-arrow-up-right-from-square"></i>
						</div>
					</a>

					<a href="https://stahlwalker.github.io/superhero-click/" target="_blank" rel="noopener noreferrer" class="project-row">
						<div class="project-square"></div>
						<div class="project-info">
							<h3 class="project-name">Clicky Game</h3>
							<p class="project-desc">Memory game using React to manage component state and user events</p>
						</div>
						<div class="project-arrow">
							<i class="fa-solid fa-arrow-up-right-from-square"></i>
						</div>
					</a>

					<a href="https://stahlwalker.github.io/TriviaGame/" target="_blank" rel="noopener noreferrer" class="project-row">
						<div class="project-square"></div>
						<div class="project-info">
							<h3 class="project-name">Superhero Trivia</h3>
							<p class="project-desc">Superhero trivia game based on jQuery and DOM manipulation</p>
						</div>
						<div class="project-arrow">
							<i class="fa-solid fa-arrow-up-right-from-square"></i>
						</div>
					</a>

					<a href="https://stahlwalker.github.io/Giphyapi/" target="_blank" rel="noopener noreferrer" class="project-row">
						<div class="project-square"></div>
						<div class="project-info">
							<h3 class="project-name">Animal Giphy</h3>
							<p class="project-desc">Animal gif generator using Giphy API with animated gifs on click</p>
						</div>
						<div class="project-arrow">
							<i class="fa-solid fa-arrow-up-right-from-square"></i>
						</div>
					</a>

					<a href="https://stahlwalker.github.io/background-generator/" target="_blank" rel="noopener noreferrer" class="project-row">
