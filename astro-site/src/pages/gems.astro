	description="Gems from Luke Stahl — showcasing work across developer marketing, product strategy, and modern web development."
	canonicalUrl="https://lucasstahl.com/gems"
	ogTitle="Gems - Luke Stahl"
	ogDescription="Curated collection of developer marketing resources, articles, podcasts, and communities."
	additionalCss={["/assets/css/about.css", "/assets/css/gems.css"]}
>
	<!-- Gems Hero Section -->
	<section class="about-hero">
		<div class="section-container">
			<h1 class="about-hero-title">
				<span class="code-bracket">&lt;</span>
				Gems
				<span class="code-bracket">/&gt;</span>
			</h1>
			<p class="about-hero-subtitle">Curated developer marketing resources</p>
			<a href="#fancy-websites" class="scroll-hint">
				<span class="comment-slash">//</span> scroll.to('websites') <span class="arrow">↓</span>
			</a>
		</div>
	</section>

	<!-- Gems Content Section -->
